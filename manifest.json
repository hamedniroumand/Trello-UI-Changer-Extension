{
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "style.css" ],
      "js": [ "Converter.js", "contentScript.js" ],
      "matches": [ "https://trello.com/*" ],
      "run_at": "document_start"
   } ],
   "description": "Change Trello Direction to rtl. For all Persian speakers :)",
   "icons": {
      "128": "icon.png",
      "16": "icon.png",
      "48": "icon.png"
   },
   "manifest_version": 2,
   "name": "Mango Agency Productivity Tool",
   "page_action": {
      "default_icon": {
         "19": "icon.png",
         "38": "icon.png"
      },
      "default_title": "Mango APT"
   },
   "version": "1.0.0"
}
